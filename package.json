{
  "name": "firstframe",
  "version": "1.0.0",
  "description": "FirstFrame - Protecci√≥n de IP para contenido audiovisual en Telegram",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:bot\" \"npm run dev:webapp\" \"npm run dev:backend\"",
    "dev:bot": "tsx watch src/bot/index.ts",
    "dev:webapp": "cd webapp && npm run dev",
    "dev:backend": "tsx watch src/backend/index.ts",
    "build": "npm run build:bot && npm run build:webapp && npm run build:backend",
    "build:bot": "tsc -p tsconfig.bot.json",
    "build:webapp": "cd webapp && npm run build",
    "build:backend": "tsc -p tsconfig.backend.json",
    "start": "node dist/index.js",
    "get-spg-contract": "tsx scripts/get-spg-contract.ts"
  },
  "keywords": [
    "telegram",
    "story-protocol",
    "ip-protection",
    "blockchain",
    "puzzle-game"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@story-protocol/core-sdk": "latest",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "sharp": "^0.32.6",
    "telegraf": "^4.15.0",
    "uuid": "^9.0.1",
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
